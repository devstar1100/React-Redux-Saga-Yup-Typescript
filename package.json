{
  "name": "space-saas",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "react-scripts start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "tsc --project ./tsconfig.json --noEmit && eslint '**/**/*.{js,ts,tsx}' --quiet --fix",
    "type-check": "tsc --pretty --noEmit",
    "prettier": "prettier --write \"**/*.tsx\""
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/lab": "^5.0.0-alpha.120",
    "@mui/material": "^5.11.10",
    "@mui/system": "^5.11.7",
    "@react-google-maps/api": "^2.18.1",
    "@react-spring/web": "^9.6.1",
    "@types/node": "^16.11.45",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-scroll-to-bottom": "^4.2.0",
    "axios": "^0.27.2",
    "date-fns": "^2.29.3",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-prettier": "^4.2.1",
    "formik": "^2.2.9",
    "lodash.debounce": "^4.0.8",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-number-format": "^5.3.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.8.2",
    "react-scripts": "5.0.1",
    "react-scroll-to-bottom": "^4.2.0",
    "react-toastify": "^9.1.1",
    "react-viewport-list": "^7.0.0",
    "recharts": "^2.3.2",
    "redux": "^4.2.0",
    "redux-axios-middleware": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "typescript": "^4.7.4",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.7",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-router": "^3.0.19",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "redux-devtools-extension": "^2.13.9",
    "webpack": "^5.74.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn tsc --noEmit && yarn eslint . && yarn prettier --write ."
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,scss,md}": [
      "prettier --write '**/**/*.{js,ts,tsx}'",
      "eslint '**/**/*.{js,ts,tsx}' --quiet --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
